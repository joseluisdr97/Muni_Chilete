@foreach (var item in Model)
{

    <!--Section: Content-->
    <section class="dark-grey-text">

        <div class="row pr-lg-5">
            <div class="col-md-7 mb-4">
                <br />
                <br />
                <div class="view">
                    <img src="https://ugelpaucartambo.com/wp-content/themes/ugelthemepro/img/convocatoria.png" style="width:500px;height:300px" class="img-fluid" alt="smaple image">
                </div>

            </div>
            <div class="col-md-5 d-flex align-items-center">
                <div>

                    <h2 class="font-weight-bold mb-4">Convocatoria</h2>

                    <p>@item.Descripcion.</p>

                </div>
            </div>
        </div>

    </section>
    <!--Section: Content-->

}

